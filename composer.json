{
    "name": "sindor/laravel-code",
    "keywords": ["laravel","laravel-code","sindor-laravel","sindor-code","code","code generator","generator","easy-code"],
    "description": "Laravel Code Generator",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Sindor Suyunov",
            "email": "sindorsuyunov92@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "livewire/livewire": ">=2.0",
        "laravel/framework": ">=10"
    },
    "autoload": {
        "psr-4": {
            "Sindor\\LaravelCode\\": "src/",
            "Sindor\\LaravelCode\\App\\": "src/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sindor\\LaravelCode\\PackageServiceProvider"
            ]
        }
    }
}
